{
    "name": "aspeclg",
    "displayName": "aspeclg",
    "description": "aspecdesc",
    "version": "0.0.1",
    "publisher": "severus21",
    "engines": {
        "vscode": "^1.66.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "aspec",
                "aliases": [
                    "aspec",
                    "aspec"
                ],
                "extensions": [
                    ".spec"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "impl",
                "aliases": [
                    "impl",
                    "impl"
                ],
                "extensions": [
                    ".impl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "aspec",
                "scopeName": "source.spec",
                "path": "./syntaxes/aspec.tmLanguage.json"
            },
            {
                "language": "impl",
                "scopeName": "source.impl",
                "path": "./syntaxes/impl.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.java": "java"
                }
            }
        ]
    }
}
